{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.fontFamily": "'Droid Sans Mono', 'Droid Sans Mono Slashed for Powerline', 'Fira Code', 'Noto Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": false,
    "editor.fontSize": 14,
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.rulers": [80,120],
    "editor.snippetSuggestions": "top",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "explorer.confirmDragAndDrop": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.menus": false,
    // "http.proxy": "http://10.100.107.30:57165",
    "http.proxyStrictSSL": false,
    "indenticator.inner.showHighlight": true,
    "python.autoUpdateLanguageServer": false,
    "selectline.displayFormat": "%d Lines",
    "selectline.alignment": "right",
    "selectline.statusbarPriority": 120,
    "terminal.integrated.fontFamily": "monospace", // "'Droid Sans Mono Slashed for Powerline', 'Droid Sans Mono', 'Fira Code', 'Noto Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.inheritEnv": false,
    "typescript.surveys.enabled": false,
    "update.mode": "manual",
    "window.menuBarVisibility": "hidden",
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Solarized Dark",
    "terminal.integrated.shellArgs.linux": [
        "-l"
    ],
    "terminal.integrated.shell.linux": "/bin/bash",
    "terminal.integrated.fontFamily": "monospace", //"'Droid Sans Mono'",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.02,
    "files.exclude": {
        "**/.swap": true,
        "**/.swp": true
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.panel.defaultLocation": "right",
    "files.autoSave": "afterDelay",
    "window.zoomLevel": 0,
    "explorer.confirmDelete": false,
    "keyboard.dispatch": "keyCode",
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.terminal.paste"
    ],
    "workbench.iconTheme": "vscode-icons",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.validate": false,
    "window.restoreFullscreen": true,
    "workbench.list.horizontalScrolling": true
}